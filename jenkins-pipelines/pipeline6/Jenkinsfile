pipeline {
 agent any
 stages {
  stage('Run Tests') {
   steps {
    sh "echo stage Run Test"
   }
  }
  stage('Deploy') {
   parallel {
    stage('Dev') {
      steps {
      node('testslave') {
		sh "echo testslave"}
      node('testslave2'){
		sh "echo testslave2" }
		
     }
    }
    stage('QA') {
     steps {
      echo 'hello stable'
     }
    }
   }
  }
 }
}

