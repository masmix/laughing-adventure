try {

node {
    stage('Build') {
        try { 
    parallel testslave: {
        node('testslave') {
            checkout scm
            try {
                unstash 'app'
                sh 'make check'
            }
            finally {
                junit '**/target/*.xml'
            }
        }
    },
    testslave2: {
        node('testslave2') {
            checkout scm
            try {
                unstash 'app'
                sh 'make check'
            }
            finally {
                junit '**/target/*.xml'
            }

        }
    }
     
		sh 'exit 0'
        }
        catch (exc) {
            echo 'Something failed, I should sound the klaxons!'
            
        }
    }
}
} finally { echo 'last finaly statement' }
